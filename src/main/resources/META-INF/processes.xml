<?xml version="1.0" encoding="UTF-8" ?>

<process-application
        xmlns="http://www.camunda.org/schema/1.0/ProcessApplication"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <process-archive name="family">
        <process-engine>default</process-engine>

        <resource>family.bpmn</resource>
        <resource>form_1.form</resource>
        <resource>choose-child.form</resource>
        <resource>search.form</resource>
        <resource>select.form</resource>
        <resource>task.form</resource>
        <resource>update.form</resource>
        <resource>reports.form</resource>
        <resource>complete.form</resource>

        <properties>
            <property name="isDeleteUponUndeploy">false</property>
            <property name="isScanForProcessDefinitions">true</property>
        </properties>
    </process-archive>

</process-application>